{
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "singleAttributePerLine": false,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "trailingComma": "none",
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
